{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"start": "docker compose up -d && bun run dev",
		"db:makemigrations": "npx drizzle-kit generate",
		"db:migrate": "npx drizzle-kit migrate",
		"db:studio": "npx drizzle-kit studio",
		"createcol": "bun run src/typesense/scripts/createCollection.ts",
		"deletecol": "bun run src/typesense/scripts/deleteCollection.ts",
		"populatecol": "bun run src/typesense/scripts/populateCollection.ts",
		"deletedb": "bun run src/db/scripts/delete.ts",
		"populatedb": "bun run src/db/scripts/populate.ts"
	},
	"devDependencies": {
